{
  "manifest_version": 2,
  "name": "my URL shortener",
  "version": "1.0",
  "description": "My URL shortener",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/beastify",
  "icons": {
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "*://is.gd/*",
    "*://chart.googleapis.com/*",
    "contextMenus",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "myUrlShortener",
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    "beasts/frog.jpg",
    "beasts/turtle.jpg",
    "beasts/snake.jpg"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
